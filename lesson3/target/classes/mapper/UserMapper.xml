<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.learn.mybatis.mapper3.UserMapper">
    <select id="selectUsernameById" resultType="java.lang.String">
        SELECT username
        FROM imooc_user
        WHERE id = #{id}
    </select>
    <select id="selectUserAgeById" resultType="java.lang.Integer">
        SELECT age
        FROM imooc_user
        WHERE id = #{id}
    </select>
</mapper>